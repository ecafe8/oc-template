# Server
NODE_ENV=development
PORT=3100
LOG_LEVEL=info

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/oc_api_db

# Auth
JWT_SECRET=your-secret-key-minimum-32-characters-long
JWT_EXPIRES_IN=7d

# CORS (comma-separated for multiple origins)
CORS_ORIGINS=http://localhost:3399

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000
